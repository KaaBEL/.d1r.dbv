{
  "$schema": "https://json.schemastore.org/web-manifest.json",
  ".D1R.DBV_VERSION": "v.0.2.7",
  "dir": "ltr",
  "lang": "en",
  "name": "DroneBoi Vehicles Editor (DBVE)",
  "short_name": "D1R DBV Editor",
  "start_url": "./editor.html?app=true",
  "id": "./",
  "scope": "./",
  "theme_color": "#111111",
  "background_color": "#333333",
  "display": "standalone",
  "icons": [
    {
      "src": "./favicon.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "./favicon.svg",
      "sizes": "16x16",
      "type": "image/svg+xml",
      "purpose": "any"
    },
    {
      "src": "/.d1r.dbv/web-app-manifest-192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "maskable"
    },
    {
      "src": "/.d1r.dbv/web-app-manifest-16x16.svg",
      "sizes": "16x16",
      "type": "image/svg+xml",
      "purpose": "monochrome"
    }
  ],
  "orientation": "any",
  "categories": ["games", "development"],
  "description": "Powerfull tool to edit drones, space ships, vehicles of Droneboi games series."
}
